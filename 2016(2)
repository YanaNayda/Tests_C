#include<stdio.h>
int findNum(int num);  // Declare the function

 
void main() {
	int num = 20;
	int missing = findNum(num);
	printf("%d\n", missing);
}


int findNum(int num) {
	int answer = 0;
	if (num <= 10) {
		return 1;
	}
	int num1 = num % 10;
	int num2 = num /10%10 ;
	 
	if (!((num1%2==0 && num2%2==1)||(num2 % 2 == 0 && num1 % 2 == 1))) {
		return 0;
	}
	

	return findNum(num/10);
}
